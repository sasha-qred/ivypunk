{
  "name": "ivypunk",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "npm run lint:prettier && npm run lint:ts && npm run lint:css",
    "lint:ts": "ng lint",
    "lint:prettier": "prettier --check \"**/*.*\"",
    "lint:prettier:partial": "prettier --check",
    "lint:css": "stylelint \"**/*.css\"",
    "e2e": "ng e2e",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "9.0.0-rc.4",
    "@angular/common": "9.0.0-rc.4",
    "@angular/compiler": "9.0.0-rc.4",
    "@angular/core": "9.0.0-rc.4",
    "@angular/forms": "9.0.0-rc.4",
    "@angular/platform-browser": "9.0.0-rc.4",
    "@angular/platform-browser-dynamic": "9.0.0-rc.4",
    "@angular/router": "9.0.0-rc.4",
    "@ngrx/data": "~8.5",
    "@ngrx/effects": "~8.5",
    "@ngrx/entity": "~8.5",
    "@ngrx/store": "~8.5",
    "@ngrx/store-devtools": "~8.5",
    "rxjs": "~6.5",
    "tslib": "~1.10",
    "zone.js": "~0.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.900.0-rc.4",
    "@angular/cli": "9.0.0-rc.4",
