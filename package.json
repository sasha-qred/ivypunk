    "@angular/language-service": "9.0.0-rc.4",
    "@commitlint/cli": "~8.2",
    "@commitlint/config-conventional": "~8.2",
    "@ngrx/schematics": "~8.5",
    "@types/node": "~12.12",
    "codelyzer": "~5.2",
    "husky": "~3.1",
    "lint-staged": "~9.5",
    "prettier": "~1.19",
    "rxjs-tslint-rules": "~4.27",
    "stylelint": "~12.0",
    "stylelint-config-idiomatic-order": "~8.1",
    "stylelint-config-standard": "~19.0",
    "stylelint-declaration-block-no-ignored-properties": "~2.2",
    "stylelint-high-performance-animation": "~1.4",
    "stylelint-no-unsupported-browser-features": "~4.0",
    "stylelint-prettier": "~1.1",
    "stylelint-use-logical": "~1.1",
    "stylelint-value-no-unknown-custom-properties": "~2.0",
    "ts-node": "~8.5",
